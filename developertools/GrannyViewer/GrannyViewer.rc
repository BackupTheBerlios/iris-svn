        MENUITEM "Log Level &4",                ID_LOG_LOGLEVEL4
        MENUITEM SEPARATOR
        MENUITEM "Setup Log...",                ID_LOG_SETUPLOG
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About GrannyViewer...",      ID_APP_ABOUT
    END
END

