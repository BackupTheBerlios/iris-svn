        MENUITEM "&Shell Open",                 ID_FILE_SHELLOPEN
        MENUITEM SEPARATOR
        MENUITEM "E&xit",                       ID_APP_EXIT
    END
    POPUP "&Render"
    BEGIN
        POPUP "&Polygon"
        BEGIN
            MENUITEM "&Point",                      ID_POLYGON_POINT
            MENUITEM "&Line",                       ID_POLYGON_LINE
            MENUITEM "&Fill",                       ID_POLYGON_FILL
        END
        MENUITEM "&Texture",                    ID_RENDER_TEXTURE
        MENUITEM "&Bone",                       ID_RENDER_BONE
        MENUITEM "Bone &Name",                  ID_RENDER_BONENAME
        POPUP "Scale"
        BEGIN
            MENUITEM "Scale x10",                   ID_RENDER_SCALE_PLUS
            MENUITEM "Scale x1/10",                 ID_RENDER_SCALE_MINUS
        END
        MENUITEM "Bound Bo&x",                  ID_RENDER_BOUNDBOX
        MENUITEM "Background Color",            ID_RENDER_BACKGROUNDCOLOR
        MENUITEM "A&xis",                       ID_RENDER_AXIS
    END
    POPUP "&Model"
    BEGIN
        MENUITEM "Clear",                       ID_MODEL_CLEARMODEL
        MENUITEM "Clear Registered",            ID_MODEL_CLEARREGISTERED
        MENUITEM "&Clear All",                  ID_MODEL_CLEARALL
        MENUITEM SEPARATOR
        MENUITEM "Set &Color",                  ID_MODEL_COLOR
        MENUITEM "&Reset Color",                ID_MODEL_RESETCOLOR
        MENUITEM SEPARATOR
        MENUITEM "&Play Animation",             ID_MODEL_STARTANIMATION
        MENUITEM "Pause &Animation",            ID_MODEL_STOPANIMATION
        MENUITEM "&Stop Animation",             ID_MODEL_PAUSEANIMATION
        MENUITEM "Set Animation Speed...",      ID_RENDER_ANIMATIONSPEED
    END
    POPUP "&Log"
    BEGIN
        MENUITEM "&Clear Log",                  ID_FILE_CLEARLOG
        MENUITEM "Set Font",                    ID_LOG_SETFONT
        MENUITEM SEPARATOR
        MENUITEM "Log Level &1",                ID_LOG_LOGLEVEL1
